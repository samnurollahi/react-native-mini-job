{
  "expo": {
    "name": "کمک خرج",
    "slug": "minijob",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "ExpoLocalization_supportsRTL": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "layoutDirection": "rtl",
      "softwareKeyboardLayoutMode": "pan",
      "edgeToEdgeEnabled": true,
      "package": "com.sam.norollahi.miniJob"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-localization"
    ],
    "extra": {
      "supportsRTL": true,
      "eas": {
        "projectId": "a166ed76-be09-4b54-84fd-fb7f1dd941bb"
      }
    }
  }
}
